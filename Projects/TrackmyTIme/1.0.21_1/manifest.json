{
   "background": {
      "persistent": true,
      "scripts": [ "/libs/js/dexie.min.js", "/libs/js/moment.min.js", "/models/events.js", "/models/User.js", "/models/Conveyor.js", "core.js" ]
   },
   "browser_action": {
      "default_icon": "/img/clock_48.png",
      "default_popup": "extension.html",
      "default_title": "MSIT TIME MANAGEMENT"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com; object-src 'self'",
   "description": "Statistics of your online presence.",
   "icons": {
      "128": "/img/clock_128.png",
      "16": "/img/clock_16.png",
      "32": "/img/clock_32.png",
      "48": "/img/clock_48.png"
   },
   "manifest_version": 2,
   "name": "SiteStat",
   "offline_enabled": true,
   "permissions": [ "idle", "tabs", "storage", "unlimitedStorage", "notifications" ],
   "short_name": "SiteStat",
   "version": "0.0.1"
}
